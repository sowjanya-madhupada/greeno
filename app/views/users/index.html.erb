<div class="form-control">
	<section>
		<h1>Users: <%= User.count %></h1>
		<table class="table table-striped">
		<thead>
			<tr>
		       <th>Name</th>
		       <th>Email</th>
		       <th>Actions</th>
		       <th colspan="3"></th>
		    </tr>
			
		</thead>
		<tbody>
			<% @users.each do |user|  %>
			   <tr>
				   <td><%= user.name %></td>
				   <td><%= user.email %></td>
				   <td><u><%= link_to "View", user %></u> |
				       <u><%= link_to "Edit", edit_user_path(user) %></ul></td>
				</tr>
			<% end %>
		</tbody>
		</table>
		<%= will_paginate @user %></strong>
	</section>
</div>


			